<rdf:RDF
    xmlns = "http://www.semanticDictionary.org/dic/2018/english#"
    xmlns:eng = "http://www.semanticDictionary.org/dic/2018/english#"
    xmlns:owl ="http://www.w3.org/2002/07/owl#"
    xmlns:rdf ="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd ="http://www.w3.org/2001/XMLSchema#"> 

<owl:Ontology rdf:about="">
    <rdfs:comment>An OWL ontology for the english language</rdfs:comment>
    <rdfs:label>English Language Dictionary</rdfs:label>
</owl:Ontology>

<owl:thing rdf:ID="Word">

<!-- Search:TYPE Definitions -->
<owl:class rdf:ID="Type">
    <rdfs:subClassOf>
        <owl:objectProperty rdf:resource="#hasType"/>
    </rdfs:subClassOf>
</owl:class>

<owl:objectProperty rdf:ID="hasType">
    <rdfs:domain rdf:resource="#Type">
    <rdfs:range rdf:rsource="#Word">
</owl:objectProperty>

<owl:class rdf:ID="Noun">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>
<owl:class rdf:ID="Verb">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>
<owl:class rdf:ID="Adjective">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>/>
<owl:class rdf:ID="Pronoun">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>
<owl:class rdf:ID="Preposition">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>
<owl:class rdf:ID="Conjunction">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>
<owl:class rdf:ID="Interjection">
    <rdfs:subClassOf rdf:resource="#Type">
</owl:class>

</rdf:RDF>